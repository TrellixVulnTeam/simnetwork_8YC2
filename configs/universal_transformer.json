{
    "max_length": 100,
    "char_limit": 10,
    "scope":"UniversalTransformer",
    "num_classes":2,

    "with_char": false,
    "char_emb_dim": 20,
    "char_lstm_dim": 40,

    "batch_size": 100,
    "max_epochs": 100,
    "dropout_rate": 0.2,
    "learning_rate": 0.001,
    "optimizer": "adam",
    "lambda_l2": 0.0,
    "grad_clipper": 10.0,

    "context_layer_num": 1,
